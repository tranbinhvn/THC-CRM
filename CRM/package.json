{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\" \"npm run css:watch\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "css": "node-sass src/ -o src/dist",
    "css:watch": "npm run css && node-sass src/ -wo src/dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.3.4",
    "@angular/compiler": "~4.3.4",
    "@angular/core": "~4.3.4",
    "@angular/forms": "~4.3.4",
    "@angular/http": "~4.3.4",
    "@angular/platform-browser": "~4.3.4",
    "@angular/platform-browser-dynamic": "~4.3.4",
    "@angular/router": "~4.3.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngui/popup": "^0.5.1",
    "angular": "^1.6.6",
    "angular-cli": "^1.0.0-beta.28.3",
    "angular-froala-wysiwyg": "^2.7.3",
    "angular-in-memory-web-api": "~0.3.0",
    "angular-timeline": "^1.7.0",
    "angular-vertical-timeline": "^0.2.0",
    "angular2-modal": "^3.0.3",
    "chart.js": "^2.7.1",
    "core-js": "^2.4.1",
    "cpx": "^1.5.0",
    "ng-drag-drop": "^4.0.0",
    "ng-dropdown": "^1.0.0",
    "ng2-charts": "^1.6.0",
    "ng2-ckeditor": "^1.1.13",
    "ng2-datetime": "^1.4.0",
    "ng2-dragula": "^1.5.0",
    "ng2-dropdown": "0.0.21",
    "ng2-notify-popup": "^0.1.25",
    "ng2-opd-popup": "1.1.17",
    "ng2-popup": "^0.4.0",
    "ng2-tabs": "0.0.11",
    "ng2-timeline": "^0.1.1",
    "ng2-translate": "^5.0.0",
    "ngx-cookie-service": "^1.0.9",
    "pdfmake": "^0.1.33",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "traceur": "0.0.96",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.4",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "ng2-tree": "^2.0.0-alpha.10",
    "node-sass": "^4.5.3",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
